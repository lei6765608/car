// 未读消息提示
//step = 0;
count = 0;
timename = "";
//alert(document);
function flash_title() {

	count++;
	step++;
	if (step == 3) {
		if (count <= 4) {
			step = 1;
		} else {
			step = 0;
			document.title = '【你有新的消息】';
			return false;
			}
	}
	;
	if (step == 1) {
		document.title = '【你有新的消息】';
	}
	if (step == 2) {
		document.title = '【　　　　　　】';
	}
	timename = setTimeout("flash_title()", 1000);
}

