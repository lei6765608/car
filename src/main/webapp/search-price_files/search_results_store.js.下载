$("#type-down a").click(function() {
    var html = $(this).html();
    $(this).parent().parent().siblings('p').html("").append(html);
});
$(document).bind("click",function(e){ 
	var target = $(e.target); 
	if(target.closest(".box-search").length == 0){ 
	   $("#select").removeClass('active');
	} 
})
$(function(){
    $("#select p").click(function() {
        $('#type-down').parent().toggleClass('active');                        
    });
    $("#type-down").click(function() {
        $('#type-down').parent().removeClass('active');                        
    });
});