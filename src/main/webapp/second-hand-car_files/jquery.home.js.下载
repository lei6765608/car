$(function() {
	$('#sliders').slidesjs({
      __height: 399,
      __width: 1180,
		//navigation :false,
		play:{
			//active: true,
			auto: true,
			interval: 4000,
			swap: true
		}
	});
	$(window,document).bind("resize",function(){
		carType();
	});
	carType();
});
function carType(){
	$("#banner .box").height($("#sliders").height());
}
